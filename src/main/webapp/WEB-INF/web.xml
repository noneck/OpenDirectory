<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>BrowseServlet</servlet-name>
		<servlet-class>gov.nysenate.opendirectory.servlets.BrowseServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PersonServlet</servlet-name>
		<servlet-class>gov.nysenate.opendirectory.servlets.PersonServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>BrowseServlet</servlet-name>
		<url-pattern>/browse</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BrowseServlet</servlet-name>
		<url-pattern>/browse/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>PersonServlet</servlet-name>
		<url-pattern>/person</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PersonServlet</servlet-name>
		<url-pattern>/person/*</url-pattern>
	</servlet-mapping>
	
</web-app>
